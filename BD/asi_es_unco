--

CREATE TABLE sede (
    id_sede integer NOT NULL,
    fax_1 character varying(8),
    fax_2 character varying(8),
    fax_3 character varying(8),
    direccion character varying(100),
    id_localidad integer,
    coordenadas_x real,
    coordenadas_y real,
    telefono_1 integer,
    telefono_2 integer,
    telefono_3 integer,
    interno_1 integer,
    interno_2 integer,
    interno_3 integer,
    id_unidad_academica character varying(5)
);


ALTER TABLE public.sede OWNER TO postgres;

